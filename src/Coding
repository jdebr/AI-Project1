if __name__ == "__main__":
	import random
	graphX = []
	graphY = []
	for i in range(1,10):
		x = random.randint(1,50)
		y = random.randint(51,100)
		graphX.append(x)
		graphY.append(y)
	print("X coordinates are - ")
	print(graphX)
	print("Y coordinates are - ")
	print(graphY)
	graph = {}
	for i in range(1,20):
		graph[random.choice(graphX)] = random.choice(graphY),random.choice(graphX)
	print(graph)
	#I just thought to do some stuff and join points randomly.
	#Waiting o hear for input and errors
